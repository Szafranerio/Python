import random

n = random.randrange(1, 10)
guess = int(input('What is your guess: '))

while n != guess:
    if n < guess:
        print('Too high')
        guess = int(input('What is your guess: '))
    elif n > guess:
        print('Too high')
        guess = int(input('What is your guess: '))
    else:
        break

print('You made a good guess!')
